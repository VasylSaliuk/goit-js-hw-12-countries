{{#each this}}
<li class="one-country-card">
  <h2 class="country-title">{{name}}</h2>
  <div class="country-info-wrap">
    <div class="country-info">
      <p> <span class="card-text">Capital:</span> {{capital}}</p>
      <p> <span class="card-text">Population:</span> {{population}}</p>
      <p> <span class="card-text">Languagues:</span> </p>
      <ul>
        {{#each languages}}
        <li>{{name}}</li>
        {{/each}}
      </ul>
    </div>
    <img src="{{flag}}" alt="flag" class="flag">
  </div>
</li>
{{/each}}